{
  "name": "social-master-lite",
  "version": "1.0.0",
  "description": "Simple, lightweight social media analytics tool with AI insights",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "./scripts/start-dev.sh",
    "dev:no-migrate": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "./scripts/migrate.sh",
    "db:migrate:up": "./scripts/migrate.sh up",
    "db:migrate:down": "./scripts/migrate.sh down",
    "db:migrate:create": "./scripts/migrate.sh create",
    "db:migrate:status": "./scripts/migrate.sh status"
  },
  "keywords": [
    "social-media",
    "ai",
    "analytics",
    "instagram",
    "apify"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.14",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.0",
    "inflight": "npm:@isaacs/fs-minipass@latest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "apify-client": "^2.13.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^8.0.3",
    "openai": "^5.10.2",
    "pg": "^8.16.3",
    "redis": "^5.6.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  }
}
